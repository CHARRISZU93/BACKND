<h3 class="text-primary"> Productos Agregados </h3>
<br>
{{#if "productos.length > 0"}}
<div class="table-responsive">
    <table class="table table-dark" id="tabla">
        <tr class="text-warning">
            <th class="font-weight-bold">Nombre</th>
            <th class="font-weight-bold">Precio</th>
        </tr>
        {{#each productos}}
        <tr>
            <td id="nombre">{{this.name}}</td>
            <td id="precio">{{this.price}}</td>
        </tr>
        {{/each}}
    </table>
</div>
{{/if}}
{{#else}}
<h3 class="alert alert-danger">no se encontraron datos</h1>
    {{/else}}